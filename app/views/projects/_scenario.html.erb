<div id="scenario" style="overflow: auto;display: inline">
  <% @pads.each do |p| %>
      <div class="image-thumb">
              <a href="http://piratepad.net/<%= p.pad_url %>?fullScreen=1" target="pad-frame" onclick="changeContent(<%= p.to_json %>)"> <img src="<%= asset_path 'screenplay.gif' %>" /></a>
        <p class="property-title"><%= p.title %></p>
      </div>
  <% end %>
  <div class="image-thumb">
    <a href="/pads/new?name=<%= @project.name %>"><img src="<%= asset_path 'Plus_sign.png' %>" width="20px" id="plus-sign" /></a>
  </div>
  <div class="test" style="font-size: 20px;"></div>
</div>

