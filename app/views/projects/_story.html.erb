<div id="story" style="overflow: auto;display: inline">
  <% @frames.each do |p| %>
          <% if params[:scene_id].to_s == p.p_name %>
              <div class="image-thumb">
                <img src="<%= asset_path 'screenplay.gif' %>" />
                <p class="property-title"><%= p.p_name %></p>
              </div>
          <% end %>
  <% end %>
  <div class="image-thumb">
    <a href="/frames/new/"><img src="<%= asset_path 'Plus_sign.png' %>" width="20px" id="plus-sign" /></a>
  </div>
</div>
